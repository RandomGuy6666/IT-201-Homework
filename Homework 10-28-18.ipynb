    "import random\n",
    "class Dice:\n",
    "    def __init__(self, l):\n",
    "        self.l = l\n",
    "    def roll(self):\n",
    "        return random.choice(self.l)\n",
    "    def roll_many(self, n):\n",
    "        return [self.roll() for i in range(n)]\n",
    "dice = Dice([1,2,3,4,5,6])\n",
    "dice.roll_many(5)\n"
   ]
